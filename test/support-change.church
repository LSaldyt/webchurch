(define samps
  (mh-query
   1000 1
   (define x (+ 0 (uniform-draw (if (flip) '(0) '(0 1)))))
   x
   true))
(soft-equal (mean samps) 0.25 0.01)
#t

(define samps
  (enumeration-query
   (define x (+ 0 (uniform-draw (if (flip) '(0) '(0 1)))))
   x
   true
   )
  )
samps
((0 1) (0.75 0.25))

(define dist
  (enumeration-query 
   (define use-first? (flip)) 
   (define x (multinomial
              (if use-first? '(a b) '(b a))
              '(0.2 0.8)
              )) 
   (if use-first? 1 0) 
   (flip (if
          (equal? x 'b)
          1.0
          0.2))))
(define probs (second dist))
(soft-equal (first probs) 0.3 0.001)
#t

